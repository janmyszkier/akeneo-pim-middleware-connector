{
  "name": "spryker-eco/akeneo-pim-middleware-connector",
  "description": "Akeneo PIM to Middleware connector Module",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/tax": "^5.0.0",
    "spryker-eco/akeneo-pim": "dev-development",
    "spryker-middleware/process": "dev-dev",
    "spryker/data-import": "^1.0.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker-eco/akeneo-pim.git"
    }
  ],
  "autoload": {
    "psr-0": {
      "SprykerEco": "src/",
      "Function": "tests/"
    }
  },
  "minimum-stability": "dev",
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
